\begin_inset Formula $(E(Y_{i}|A_{i}^{s},W_{i}^{s})$
\end_inset


\end_layout

\end_deeper
\begin_layout Itemize

\series bold
TMLE
\series default
: Targeted Maximum Likelihood Estimator
\begin_inset Note Note
status open

\begin_layout Plain Layout
outside the scope of this talk
\end_layout

\end_inset


\end_layout

